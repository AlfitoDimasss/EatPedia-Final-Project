{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "import/extensions": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "object-curly-newline": 0,
        "prefer-const": 0,
        "max-len": 0
    }
}